[Unit]
Description=polardb-o universe explorer
After=network.target

[Service]
Type=forking
PIDFile=/run/pdb_ue.pid
ExecStart=/opt/pdb_ue/bin/service_systemd.sh start
ExecReload=/opt/pdb_ue/bin/service_systemd.sh restart
ExecStop=/opt/pdb_ue/bin/service_systemd.sh stop
Restart=always
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target
